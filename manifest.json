{
  "manifest_version": 2,

  "name": "Dexcom Uploader",
  "description": "Report #cgmnow, run reports, and upload data to #diyps or NightScout.",
  "version": "0.1.0.1",
  "app": {
    "background": {
      "scripts": ["app/background.js"]
    }
  },
  "permissions": [
    "serial",
    "storage",
    "<all_urls>",
    "notifications"
  ],
  "icons": {
    "16": "public/assets/icon.png",
    "128": "public/assets/icon.png"
  }
}
